{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "customManagers": [
    {
      "customType": "regex",
      "description": ["Process any raw.githubusercontent.com files"],
      "fileMatch": ["(^|/)argocd/.+\\.ya?ml$"],
      "matchStrings": ["https:\/\/raw\\.githubusercontent\\.com\/(?<depName>[^\\s:]+)\/v?(?<major>\\d+)\\.(?<minor>\\d+)\\.(?<patch>\\d+)\/\\S+"],
      "datasourceTemplate": "github-releases"
    }
  ]
}
